<ns4:userStoreDTO xmlns:ns4="http://org.apache.axis2/xsd">
    <ns1:className xmlns:ns1="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">org.wso2.carbon.identity.user.store.remote.CarbonRemoteUserStoreManger</ns1:className>
    <ns1:description xmlns:ns1="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">Remote User Store</ns1:description>
    <ns1:domainId xmlns:ns1="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">solution-06-remote</ns1:domainId>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>remoteUserName</name>
        <value>admin@bar.com</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>password</name>
        <value>admin</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>serverUrls</name>
        <value>https://localhost:9443/services</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>PasswordJavaScriptRegEx</name>
        <value>^[\S]{5,30}$</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>UserNameJavaScriptRegEx</name>
        <value>^[\S]{3,30}$</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>RoleNameJavaScriptRegEx</name>
        <value>^[\S]{3,30}$</value>
    </properties>
    <properties xmlns="http://dto.configuration.store.user.identity.carbon.wso2.org/xsd">
        <name>Disabled</name>
        <value>false</value>
    </properties>
</ns4:userStoreDTO>