<ns4:spDto xmlns:ns4="http://org.apache.axis2/xsd">
    <ns1:assertionConsumerUrls xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://localhost:8080/solution-19-francesca.com/samlcallback</ns1:assertionConsumerUrls>
    <ns1:assertionEncryptionAlgorithmURI xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://www.w3.org/2001/04/xmlenc#aes256-cbc</ns1:assertionEncryptionAlgorithmURI>
    <ns1:attributeConsumingServiceIndex xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd"></ns1:attributeConsumingServiceIndex>
    <ns1:certAlias xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">wso2carbon</ns1:certAlias>
    <ns1:defaultAssertionConsumerUrl xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://localhost:8080/solution-19-francesca.com/samlcallback</ns1:defaultAssertionConsumerUrl>
    <ns1:digestAlgorithmURI xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://www.w3.org/2000/09/xmldsig#sha1</ns1:digestAlgorithmURI>
    <ns1:doSignAssertions xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">true</ns1:doSignAssertions>
    <ns1:doSignResponse xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">false</ns1:doSignResponse>
    <ns1:doValidateSignatureInRequests xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">false</ns1:doValidateSignatureInRequests>
