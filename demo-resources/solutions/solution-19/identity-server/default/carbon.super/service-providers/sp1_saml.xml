    <ns1:keyEncryptionAlgorithmURI xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://www.w3.org/2001/04/xmlenc#rsa-oaep-mgf1p</ns1:keyEncryptionAlgorithmURI>
    <ns1:nameIDFormat xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">urn/oasis/names/tc/SAML/1.1/nameid-format/emailAddress</ns1:nameIDFormat>
    <ns1:signingAlgorithmURI xmlns:ns1="http://dto.saml.sso.identity.carbon.wso2.org/xsd">http://www.w3.org/2000/09/xmldsig#rsa-sha1</ns1:signingAlgorithmURI>
</ns4:spDto>