<ns4:application xmlns:ns4="http://org.apache.axis2/xsd">
    <ns1:OAuthVersion xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">OAuth-2.0</ns1:OAuthVersion>
    <ns1:applicationAccessTokenExpiryTime xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">3600</ns1:applicationAccessTokenExpiryTime>
    <ns1:callbackUrl xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">http://localhost:8080/solution-02-lebens.com/callback</ns1:callbackUrl>
    <ns1:grantTypes xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">refresh_token urn:ietf:params:oauth:grant-type:saml2-bearer implicit password client_credentials iwa:ntlm authorization_code urn:ietf:params:oauth:grant-type:jwt-bearer </ns1:grantTypes>
    <ns1:pkceMandatory xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">false</ns1:pkceMandatory>
    <ns1:pkceSupportPlain xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">true</ns1:pkceSupportPlain>
    <ns1:refreshTokenExpiryTime xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">84600</ns1:refreshTokenExpiryTime>
    <ns1:userAccessTokenExpiryTime xmlns:ns1="http://dto.oauth.identity.carbon.wso2.org/xsd">3600</ns1:userAccessTokenExpiryTime>
</ns4:application>